{
  "name": "clapy",
  "version": "1.0.0",
  "description": "Figma plugin to start building Clapy applications.",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "start": "webpack --mode=development --watch",
    "dev": "npm-run-all --parallel dev-with-browser:plugin dev-with-browser:browser dev-with-browser:server",
    "build": "webpack --mode=production --env APP_ENV=staging && rimraf dist && mkdirp dist/build && ncp manifest.json dist/manifest.json && ncp build/index.html dist/build/index.html && ncp build/backend.js dist/build/backend.js && cd dist && bestzip plugin.zip *",
    "upgr": "yarn upgrade-interactive --latest",
    "_________________ implementation details _________________": "",
    "dev-with-browser:plugin": "webpack --mode=development --env PREVIEW_ENV=figma --env APP_ENV=dev --watch",
    "dev-with-browser:browser": "webpack-dev-server --mode=development --env PREVIEW_ENV=browser --env APP_ENV=dev",
    "dev-with-browser:server": "nodemon --watch src/core/dev-preview-mode/preview-server.js src/core/dev-preview-mode/preview-server.js",
    "clean": "sudo kill -9 `sudo lsof -t -i:9001` && sudo kill -9 `sudo lsof -t -i:9000`",
    "lint": "eslint src --ext .js,.jsx,ts,tsx --fix"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.7.1",
    "buffer": "^6.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "sha.js": "^2.4.11"
  },
  "devDependencies": {
    "@figma/plugin-typings": "^1.41.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.4",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/sha.js": "^2.4.0",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "bestzip": "^2.2.0",
    "css-loader": "^6.5.1",
    "csstype": "^3.0.10",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.2",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "fork-ts-checker-webpack-plugin": "^6.5.0",
    "html-webpack-plugin": "^5.5.0",
    "mkdirp": "^1.0.4",
    "ncp": "^2.0.0",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.1",
    "react-dev-utils": "^12.0.0",
    "react-refresh": "^0.11.0",
    "react-refresh-typescript": "^2.0.3",
    "rimraf": "^3.0.2",
    "sass": "^1.49.0",
    "sass-loader": "^12.4.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.66.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.3",
    "ws": "^8.4.2"
  }
}
